pipeline {
	agent any

	stages {
		stage('build') {
			environment {
				TC='/usr/bin/avr'
				TOOLS_ROOT='/var/lib/jenkins/Arc/toolchain'
			}
			steps {
				sh 'make demo_avr clean default'
			}
		}
		stage('clean') {
			environment {
				TC='/usr/bin/avr'
				TOOLS_ROOT='/var/lib/jenkins/Arc/toolchain'
			}
			steps {
				sh 'make demo_avr clean'
			}
		}
	}
}
